/* Problem 5 */

/* (a) */

SELECT courseID,courseName FROM COURSES;

/* (b) */

SELECT studentName,courseID FROM STUDENT WHERE studentGrade = 'I';

/* (c) */

SELECT facultyName AS Name, facultyQualification AS Qualification FROM FACULTY,COURSES WHERE COURSES.courseID = FACULTY.courseID AND COURSES.courseName LIKE 'D%';

/* (d) */

SELECT * FROM STUDENT WHERE studentYear = '1'
UNION
SELECT * FROM STUDENT WHERE studentYear = '4';

/* (e) */

SELECT studentName FROM STUDENT WHERE courseID = 'CSD204' IN (SELECT studentName FROM STUDENT WHERE courseID = 'CSD202');

/* (f) */

SELECT * FROM COURSES,STUDENT;

/* (g) */

SELECT studentName, studentYear, studentMajor FROM STUDENT,COURSES WHERE COURSES.courseID = STUDENT.courseID AND COURSES.courseName IN ('DATA STRUCTURES','OPERATING SYSTEMS');

/* (h) */

SELECT * FROM FACULTY WHERE facultyName LIKE 'D%';

/* (i) */

SELECT studentName AS Name FROM STUDENT WHERE studentYear BETWEEN 2 AND 4;



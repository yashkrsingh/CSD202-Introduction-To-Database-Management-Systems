/* Problem 1 */

CREATE TABLE COURSES(
courseID VARCHAR(6) PRIMARY KEY,
courseName VARCHAR(50) NOT NULL,
courseDept VARCHAR(25) NOT NULL,
courseSchool VARCHAR(50) NOT NULL
);

/* Problem 2 */

CREATE TABLE FACULTY(
courseID VARCHAR(6),
facultyName VARCHAR(30) NOT NULL,
facultyPosition VARCHAR(30) NOT NULL,
facultyQualification VARCHAR(30) NOT NULL DEFAULT 'UNKNOWN',
FOREIGN KEY (courseID) REFERENCES COURSES(courseID)
);

/* Problem 3 */

CREATE TABLE STUDENT(
courseID VARCHAR(6),
studentName VARCHAR(30) NOT NULL,
studentYear INT(1) NOT NULL,
studentMajor VARCHAR(30) NOT NULL,
studentGrade VARCHAR(1) NOT NULL DEFAULT 'I',
FOREIGN KEY (courseID) REFERENCES COURSES(courseID)
);

